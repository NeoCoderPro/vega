  "main": "build/vega.js",
  "jsnext:main": "index",
  "repository": {
    "type": "git",
    "url": "https://github.com/uwdata/vega.git"
  },
  "scripts": {
    "build": "npm run test && uglifyjs build/vega.js -c -m -o build/vega.min.js",
    "pretest": "rm -rf build && mkdir build && json2module package.json > build/package.js && bin/rollup",
    "test": "tape 'test/**/*-test.js' && eslint index.js src test"
  },
  "dependencies": {
    "vega-dataflow": "2.0.0",
    "vega-expression": "2.0.0",
    "vega-parser": "1.0.0",
    "vega-runtime": "1.0.0",
    "vega-scenegraph": "2.0.0"
  },
  "devDependencies": {
    "eslint": "2",
    "json2module": "0.0",
    "rollup": "0.26",
    "rollup-plugin-node-resolve": "1",
    "tape": "4",
    "uglify-js": "2"
  }
}