  "keywords": [
    "vega",
    "wordcloud",
    "layout"
  ],
  "license": "BSD-3-Clause",
  "author": "Jeffrey Heer (http://idl.cs.washington.edu)",
  "main": "build/vega-wordcloud.js",
  "module": "build/vega-wordcloud.module.js",
