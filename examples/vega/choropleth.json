{
  "width": 960,
  "height": 500,
  "data": [
    {
      "name": "unemp",
      "url": "data/unemployment.tsv",
      "format": {"type":"tsv", "parse":{"rate":"number"}}
    },
    {
      "name": "counties",
      "url": "data/us-counties.geojson",
      "format": {"type": "json", "property": "features"},
      "transform": [
        {"type": "geopath", "value": "data", "projection": "albersUsa"},
        {
          "type": "zip",
          "key": "data.id",
          "with": "unemp",
          "withKey": "data.id",
          "as": "value",
          "default": {"data":{"id":0,"rate":0}}
        }
      ]
    }
  ],
  "scales": [
    {
      "name": "color",
      "type": "quantize",
      "domain": [0, 0.15],
      "range": ["#f7fbff", "#deebf7", "#c6dbef", "#9ecae1", "#6baed6",
                "#4292c6", "#2171b5", "#08519c", "#08306b"]
    }
  ],
  "marks": [
    {
      "type": "path",
      "from": {"data": "counties"},
      "properties": {
        "enter": {
          "path": {"field": "path"}
        },
        "update": {
          "fill": {"scale": "color", "field": "value.data.rate"}
        },
        "hover": {
          "fill": {"value": "red"}
        }
      }
    }
  ]
}